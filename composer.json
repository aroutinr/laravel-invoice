{
    "name": "aroutinr/laravel-invoice",
    "description": "Create invoice for any model of your Laravel App",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Richard Aroutin",
            "email": "raroutin@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "AroutinR\\Invoice\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AroutinR\\Invoice\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AroutinR\\Invoice\\InvoiceServiceProvider"
            ],
            "aliases": {
                "CreateInvoice": "AroutinR\\Invoice\\Facades\\CreateInvoice",
                "CreatePayment": "AroutinR\\Invoice\\Facades\\CreatePayment"
            }
        }
    },
    "require": {
        "php": "^8.0|^8.1|^8.2",
        "illuminate/support": "^8.0|^9.0|^10.0"
    },
    "require-dev": {
        "orchestra/testbench": ">=4.0",
        "phpunit/phpunit": ">=8.5",
        "laravel/legacy-factories": "^1.1"
    }
}
